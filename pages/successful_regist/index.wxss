.container {
  width: 710rpx;
  height: 800rpx;
  margin: 120rpx auto 0;
}
.container .title {
  font-size: 26rpx;
}
.container view {
  display: flex;
  justify-content: center;
  color: #777777;
}
.container .img_area {
  height: 300rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.container .img_area image {
  width: 200rpx;
  height: 200rpx;
}
.container .btn {
  width: 600rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  border-radius: 20rpx;
  margin: 30rpx auto;
}
.container .btn text {
  font-size: 34rpx;
  padding-top: 4rpx;
}
.container .btn_home {
  background-color: #dbd2d2;
  color: #fff;
  margin-top: 60rpx;
}
.container .btn_wx_area {
  background-color: #029400;
  color: #fff;
}
.area {
  width: 600rpx;
  height: 150rpx;
  margin: 100rpx auto;
}
.area .btn {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
}
.area .btn button {
  color: #88D0FF;
}
.area .content {
  width: 92%;
  margin: 20rpx auto;
  line-height: 45rpx;
  text-align: left;
}
